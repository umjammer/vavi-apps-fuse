        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </snapshots>
    </repository>
    <repository>
      <id>de.tuberlin</id>
      <url>https://raw.github.com/tawalaya/onedrivejavasdk/mvn-repo/</url>
