<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<configuration>
  <appender name="STDERR" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.jayway.jsonpath" level="OFF"/>

  <root level="OFF">
    <appender-ref ref="STDERR" />
  </root>
</configuration>